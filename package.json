{
  "name": "express-pg-exercise",
  "version": "1.0.0",
  "description": "playground for express and postgresql",
  "author": "Dragan Nakevski",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "serve": "node dist/index.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.14.11",
    "express": "^4.19.2",
    "module-alias": "^2.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@types/express-validator": "^3.0.0",
    "body-parser": "^1.20.2",
    "express-validator": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "zod": "^3.23.8"
  },
  "_moduleAliases": {
    "@root": ".",
    "@admin": "src/admin",
    "@admin/*": "src/admin/*",
    "@standard-user": "src/standard-user",
    "@standard-user/*": "src/standard-user/*",
    "@middleware": "src/middleware",
    "@middleware/*": "src/middleware/*",
    "@schemas": "src/schemas",
    "@schemas/*": "src/schemas/*"
  }
}
